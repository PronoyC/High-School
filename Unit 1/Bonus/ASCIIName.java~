public class ASCIIName
{
 public static void main (String args[])
 {
 new ASCIIName ();
 }
 public ASCIIName ()
 {
System.out.println ("         _____                    _____                   _______                   _____                   _______               _____");          
System.out.println ("        /\\    \\                  /\\    \\                 /::\\    \\                 /\\    \\                 /::\\    \\             |\\    \\");         
